{
    "manifest_version": 3,
    "name": "TaskTrack",
    "version": "1.0",
    "description": "A local Chrome extension for managing tasks with priorities and date navigation.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/TaskTrack_icon_16x16.png",
        "48": "icons/TaskTrack_icon_48x48.png",
        "128": "icons/TaskTrack_icon_128x128.png"
      }
    }
  }
  